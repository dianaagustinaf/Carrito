@{
    ViewData["Title"] = "Crear Usuarios";
}

<h1>Crear</h1>

<form asp-controller="Usuarios" asp-action="Crear" method="post">
    <span>Nombre: </span>
    <input type="text" name="nombre" />

    <span>Apellido: </span>
    <input type="text" name="apellido" />

    <span>DNI: </span>
    <input type="text" name="dni" />

    <button type="submit">Enviar</button>

</form>